  "homepage": "https://github.com/patrickfatrick/slogged#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
